<h1> Please log in. </h1>
<%= form_for @user, url: login_path, method: :post do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>

    <%= f.label :email %>
    <%= f.email_field :email %> <br><br>   
  
    <%= f.label :password %>
    <%= f.password_field :password %> <br><br>
  
    <%= f.submit "Log In" %>
<% end %>